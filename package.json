{
  "name": "commi-dashboard",
  "version": "1.0.0",
  "private": true,
  "description": "Community dashboard system with Twitter data analysis and NFT rewards",
  "author": "Your Name",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "pnpm -r --parallel run dev",
    "build": "pnpm -r run build",
    "lint": "pnpm -r run lint",
    "format": "pnpm -r run format",
    "test": "pnpm -r run test",
    "clean": "pnpm -r run clean && rimraf node_modules",
    "db:generate": "pnpm --filter @commi-dashboard/db generate",
    "db:push": "pnpm --filter @commi-dashboard/db push",
    "db:migrate": "pnpm --filter @commi-dashboard/db migrate:dev",
    "db:migrate:prod": "pnpm --filter @commi-dashboard/db migrate:prod",
    "web:dev": "pnpm --filter @commi-dashboard/web dev",
    "web:build": "pnpm --filter @commi-dashboard/db generate && pnpm --filter @commi-dashboard/web build",
    "web:start:invite": "pnpm --filter @commi-dashboard/web start:invite",
    "web:start:dev:invite": "pnpm --filter @commi-dashboard/web start:dev:invite",
    "backend": "pnpm --filter @commi-dashboard/backend dev",
    "dashboard": "pnpm --filter @commi-dashboard/dashboard dev",
    "dashboard:build": "pnpm --filter @commi-dashboard/dashboard build",
    "dashboard:start": "pnpm --filter @commi-dashboard/dashboard start",
    "prepare": "husky install"
  },
  "dependencies": {
    "next": "15.3.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,mdx,css,scss}": [
      "prettier --write"
    ]
  },
  "packageManager": "pnpm@10.11.1+sha1.82b5e196a3201a1871fce7461b3bdf8742486168"
}
